<div class="row">
	<div class="span8 offset3">
		<%= form_for(@article) do |f| %>

			<%= render 'shared/error_messages', object: f.object %>

			<%= f.label "Title" %>
			<%= f.text_field :title %>

			<%= f.label "Content" %>
			<%= f.text_field :content %>

			<%= f.submit "Create Article", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>


	
